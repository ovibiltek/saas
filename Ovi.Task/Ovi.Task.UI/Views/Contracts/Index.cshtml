@using Resources.Contracts.Index
@{
    Layout = "~/Views/Shared/Default.cshtml";
}

@Html.IncludeVersionedCss("/Content/jquery/jquery.fileupload.css")

@Html.IncludeVersionedCss("/Content/jquery/jquery.contextMenu.min.css")


<div style="margin-top: 10px">
    <div class="page-header">
        <h4>
            @ContractStrings.contracts
        </h4>
        <h6></h6>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="tms-page-toolbar inline-toolbar left" data-block="#record">
                <div class="btn-group">
                    <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" id="btnNew"><i class="fa fa fa-plus fa-fw"></i>@ContractStrings.addnew</button>
                    <button class="btn btn-default btn-sm" disabled="disabled" selection-required="false" title="Ctrl+S" id="btnSave"><i class="fa fa-floppy-o fa-fw"></i>@ContractStrings.save</button>
                    <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+U" id="btnUndo"><i class="fa fa-undo fa-fw"></i>@ContractStrings.undo</button>
                    <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+D" id="btnDelete"><i class="fa fa-trash-o fa-fw"></i>@ContractStrings.delete</button>
                    <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+H" id="btnHistory"><i class="fa fa-bolt fa-fw"></i>@ContractStrings.history</button>
                </div>
            </div>
        </div>
    </div>
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#list">@ContractStrings.list</a></li>
        <li><a data-toggle="tab" href="#record">@ContractStrings.record</a></li>
        <li><a enable-on-selection href="#pricing">@ContractStrings.pricing</a></li>
        <li><a enable-on-selection href="#partprices">@ContractStrings.partprices</a></li>
        <li><a enable-on-selection href="#serviceprices">@ContractStrings.serviceprices</a></li>
        <li><a enable-on-selection href="#contractequipmanprices">@ContractStrings.contractequipmanprices</a></li>

    </ul>
    <div class="tab-content">
        <div id="list" class="tab-pane active">
            <div id="grdContracts"></div>
        </div>
        <div id="record" class="tab-pane">
            <div class="row record-content">
                <div class="col-md-4" id="divHeader">
                    <div class="panel panel-default" style="height: 842px;">
                        <div class="panel-heading task-panel-heading">@ContractStrings.record</div>
                        <div class="panel-body task-panel-body">
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.code
                                </div>
                                <div class="col-md-8">
                                    <input id="code" disabled="disabled" alwaysdisabled="yes" type="text" class="form-control uppercase" />
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.organization
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text" loadstatusesonchange="yes" disableonupdate="yes" maxlength="50" required class="form-control uppercase" id="org">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btnorg"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.desc
                                </div>
                                <div class="col-md-8">
                                    <input type="text" maxlength="250" class="form-control" required id="desc" />
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.details
                                </div>
                                <div class="col-md-8">
                                    <textarea id="details" maxlength="1000" style="width: 100%; height: 100px;"></textarea>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.referenceno
                                </div>
                                <div class="col-md-8">
                                    <input type="text" maxlength="50" class="form-control" id="referenceno" />
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.type
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text" loadstatusesonchange="yes" disableonupdate="yes" maxlength="50" required data-entity="CONTRACT" class="form-control uppercase" id="type">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btntype"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.status
                                </div>
                                <div class="col-md-8">
                                    <select class="form-control" data-code="-" data-pcode="Q" data-text="" required id="status"></select>
                                </div>
                            </div>
                            <div id="divcancellationreason" class="hidden">
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ContractStrings.cancellationreason
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" id="cancellationreason">
                                            <option class="default" value="">@ContractStrings.pleaseselect</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div id="divcustomer" class="row custom hidden">
                                <div class="col-md-4">
                                    @ContractStrings.customer
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text" disableonupdate="yes" maxlength="50" required class="form-control uppercase" id="customer">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btncustomer"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div id="divsupplier" class="row custom hidden">
                                <div class="col-md-4">
                                    @ContractStrings.supplier
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text" disableonupdate="yes" maxlength="50" required class="form-control uppercase" id="supplier">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btnsupplier"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.manager
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text" maxlength="50" class="form-control uppercase" id="manager">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button" id="btnmanager"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.startdate
                                </div>
                                <div class="col-md-8">
                                    <input type="text" required ctrltype="datepicker" class="form-control" id="startdate">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.enddate
                                </div>
                                <div class="col-md-8">
                                    <input type="text" required ctrltype="datepicker" class="form-control" id="enddate">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.remindingperiod
                                </div>
                                <div class="col-md-8">
                                    <input type="text" ctrltype="numeric" allowfloat="false" maxlength="250" class="form-control" id="remindingperiod">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.paymentdue
                                </div>
                                <div class="col-md-8">
                                    <input type="text" ctrltype="numeric" allowfloat="false" maxlength="250" class="form-control" id="paymentdue">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.createdby
                                </div>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" disabled="disabled" id="createdby" />
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ContractStrings.created
                                </div>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" disabled="disabled" id="created" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-4" id="divCustomFields">
                    <div class="panel panel-default" style="height: 842px;">
                        <div class="panel-heading task-panel-heading">@ContractStrings.customfields</div>
                        <div class="panel-body">
                            <div id="cfcontainer"></div>
                        </div>
                    </div>
                </div>
                <div class="col-md-4" id="divCmntsAttachments">
                    <div class="panel panel-default" style="height: 450px;">
                        <div class="panel-heading task-panel-heading">@ContractStrings.comments</div>
                        <div class="panel-body">
                            <textarea class="form-control" contenteditable rows="3" id="comment"></textarea>
                            <div class="inline-toolbar">
                                <button class="btn btn-primary btn-xs" selection-required="true" id="addComment"><i class="fa fa-plus-circle"></i>@ContractStrings.add</button>
                            </div>
                            <div id="comments" class="sm">
                            </div>
                        </div>
                    </div>
                    <div class="panel panel-default" style="height: 372px;">
                        <div class="panel-heading task-panel-heading">@ContractStrings.docs</div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="progress">
                                        <div class="progress-bar" id="docuprogress" role="progressbar" style="width: 0%; min-width: 2em;">0%</div>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-12">
                                    <div id="fupload">
                                        <span class="btn btn-default fileinput-button" id="btnBrowse">
                                            <i class="fa fa-search"></i>
                                            <span>@ContractStrings.browse</span>
                                            <input type="file" name="files[]" id="fu" multiple data-url="/UploadFile.ashx">
                                        </span>
                                        <button class="btn btn-default" id="btnupload" disabled="disabled"><i class="fa fa-cloud-upload"></i>@ContractStrings.upload</button>
                                        <div class="row custom">
                                            <div class="col-md-12">
                                                <div id="filename"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="docs" class="light-size"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="pricing" class="tab-pane">
            <div class="row">
                <div class="col-md-12">
                    <div id="grdPricing"></div>
                </div>
            </div>
            <br />
            <div class="panel panel-default">
                <div class="row">
                    <div class="col-md-12" style="padding-right:0">
                        <div class="inline-toolbar left" style="border:none;" data-block="#exch">
                            <div class="btn-group">
                                <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" id="btnAddPricing"><i class="fa fa fa-plus fa-fw"></i>@ContractStrings.addnew</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+S" selection-required="false" id="btnSavePricing"><i class="fa fa-floppy-o fa-fw"></i>@ContractStrings.save</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+D" selection-required="true" id="btnDeletePricing"><i class="fa fa-trash-o fa-fw"></i>@ContractStrings.delete</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel-body page-panel-body">
                    <div class="row">
                        <div class="col-md-6">
                            <div id="divallbranches" class="row custom hidden">
                                <div class="col-md-3">
                                    @ContractStrings.allbranches
                                </div>
                                <div class="col-md-6">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled" disableonupdate="yes" checked="checked" id="allbranches" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                            <div id="divbranch" class="row custom hidden">
                                <div class="col-md-3">
                                    @ContractStrings.branch
                                </div>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <input type="text" disableonupdate="yes" maxlength="50" class="form-control uppercase" id="branch">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btnbranch"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-3">
                                    @ContractStrings.pricingcode
                                </div>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <input type="text" maxlength="50" disableonupdate="yes" class="form-control" required id="pricingcode">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btnpricingcode"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-3">
                                    @ContractStrings.startdate
                                </div>
                                <div class="col-md-6">
                                    <input type="text" class="form-control" id="pricingstartdate">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-3">
                                    @ContractStrings.enddate
                                </div>
                                <div class="col-md-6">
                                    <input type="text" class="form-control" id="pricingenddate">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-3">
                                    @ContractStrings.taskcategory
                                </div>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <input type="text" required class="form-control" id="taskcategory">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button" id="btntaskcategory"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom hidden" id="divperiodictask">
                                <div class="col-md-3">
                                    @ContractStrings.periodictask
                                </div>
                                <div class="col-md-6">
                                    <div class="input-group">
                                        <input type="text" class="form-control" id="periodictask">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button" id="btnperiodictask"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-3">
                                    @ContractStrings.remindingperiod
                                </div>
                                <div class="col-md-6">
                                    <input type="text" ctrltype="numeric" allowfloat="false" maxlength="250" class="form-control" id="pricingremindingperiod">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-3">
                                    @ContractStrings.note
                                </div>
                                <div class="col-md-6">
                                    <textarea id="note" style="width: 100%; height: 100px;"></textarea>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div id="pricingparameterdetails"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="partprices" class="tab-pane">
            <div class="row">
                <div class="col-md-12">
                    <div id="grdPartPrices"></div>
                </div>
            </div>
            <br />
            <div class="panel panel-default">
                <div class="row">
                    <div class="col-md-12" style="padding-right: 0">
                        <div class="inline-toolbar left" style="border: none;" data-block="#pricing">
                            <div class="btn-group">
                                <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" id="btnAddPartPrice"><i class="fa fa fa-plus fa-fw"></i>@ContractStrings.addnew</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+S" selection-required="false" id="btnSavePartPrice"><i class="fa fa-floppy-o fa-fw"></i>@ContractStrings.save</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+D" selection-required="false" id="btnDeletePartPrice" ><i class="fa fa-trash-o fa-fw"></i>@ContractStrings.delete</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel-body page-panel-body">
                    <div class="row custom">
                        <div class="col-md-2">
                            @ContractStrings.part
                        </div>
                        <div class="col-md-3">
                            <div class="input-group">
                                <input type="text" maxlength="50" disableonupdate="yes" class="form-control" required id="part">
                                <span class="input-group-btn">
                                    <button class="btn btn-default" disableonupdate="yes" type="button" id="btnpart"><i class="fa fa-search fa-fw"></i></button>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="row custom">
                        <div class="col-md-2">
                            @ContractStrings.partdesc
                        </div>
                        <div class="col-md-3">
                            <input type="text" disabled="disabled" class="form-control" id="partdesc">
                        </div>
                    </div>
                    <div class="row custom">
                        <div class="col-md-2">
                            @ContractStrings.region
                        </div>
                        <div class="col-md-3">
                            <div class="input-group">
                                <input type="text" maxlength="50"  class="form-control" id="cppregion">
                                <span class="input-group-btn">
                                    <button class="btn btn-default" type="button" id="btncppregion"><i class="fa fa-search fa-fw"></i></button>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="row custom">
                        <div class="col-md-2">
                            @ContractStrings.branch
                        </div>
                        <div class="col-md-3">
                            <div class="input-group">
                                <input type="text" maxlength="50" class="form-control" id="cppbranch">
                                <span class="input-group-btn">
                                    <button class="btn btn-default" type="button" id="btncppbranch"><i class="fa fa-search fa-fw"></i></button>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="row custom">
                        <div class="col-md-2">
                            @ContractStrings.reference
                        </div>
                        <div class="col-md-3">
                            <input type="text" class="form-control" id="reference">
                        </div>
                    </div>
                    <div class="row custom">
                        <div class="col-md-2">
                            @ContractStrings.partuom
                        </div>
                        <div class="col-md-3">
                            <input type="text" disabled="disabled" class="form-control" id="partuom">
                        </div>
                    </div>
                    <div class="row custom">
                        <div class="col-md-2">
                            @ContractStrings.unitpurchaseprice
                        </div>
                        <div class="col-md-3">
                            <input type="text" valtype="PRICE" ctrltype="numeric" allowfloat="true" class="form-control" id="partunitpurchaseprice">
                        </div>
                    </div>
                    <div class="row custom">
                        <div class="col-md-2">
                            @ContractStrings.unitsalesprice
                        </div>
                        <div class="col-md-3">
                            <input type="text" required valtype="PRICE" ctrltype="numeric" allowfloat="true" class="form-control" id="partunitsalesprice">
                        </div>
                    </div>
                    <div class="row custom">
                        <div class="col-md-2">
                            @ContractStrings.currency
                        </div>
                        <div class="col-md-3">
                            <div class="input-group">
                                <input type="text" required maxlength="50" class="form-control" id="currency">
                                <span class="input-group-btn">
                                    <button class="btn btn-default" type="button" id="btncurrency"><i class="fa fa-search fa-fw"></i></button>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="serviceprices" class="tab-pane">
            <div class="row">
                <div class="col-md-12">
                    <div id="grdServicePrices"></div>
                </div>
            </div>
            <br />
            <div class="panel panel-default">
                <div class="row">
                    <div class="col-md-12" style="padding-right: 0">
                        <div class="inline-toolbar left" style="border: none;" data-block="#pricing">
                            <div class="btn-group">
                                <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" id="btnAddServicePrices"><i class="fa fa fa-plus fa-fw"></i>@ContractStrings.addnew</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+S" selection-required="false" id="btnSaveServicePrices"><i class="fa fa-floppy-o fa-fw"></i>@ContractStrings.save</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+D" selection-required="false" id="btnDeleteServicePrices"><i class="fa fa-trash-o fa-fw"></i>@ContractStrings.delete</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel-body page-panel-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="row custom">
                                <div class="col-md-2">
                                    @ContractStrings.servicecode
                                </div>
                                <div class="col-md-3">
                                    <div class="input-group">
                                        <input type="text" maxlength="50" disableonupdate="yes" class="form-control" required id="servicecode">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btnservicecode"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @ContractStrings.region
                                </div>
                                <div class="col-md-3">
                                    <div class="input-group">
                                        <input type="text" maxlength="50" class="form-control" id="cspregion">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button" id="btncspregion"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @ContractStrings.branch
                                </div>
                                <div class="col-md-3">
                                    <div class="input-group">
                                        <input type="text" maxlength="50" class="form-control" id="cspbranch">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button" id="btncspbranch"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @ContractStrings.reference
                                </div>
                                <div class="col-md-3">
                                    <input type="text" class="form-control" id="servicereference">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @ContractStrings.unitpurchaseprice
                                </div>
                                <div class="col-md-3">
