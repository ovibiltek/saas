            <div class="modal-footer">
                <button type="button" data-dismiss="modal" class="btn btn-success btn-xs" id="ok"><i class="fa fa-check-circle"></i> @PeriodicTaskStrings.ok</button>
                <button type="button" data-dismiss="modal" class="btn btn-xs" id="cancel"><i class="fa fa-close"></i> @PeriodicTaskStrings.cancel</button>
