@using Resources.PeriodicTasks.Index
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Default.cshtml";
}

@Html.IncludeVersionedCss("/Content/bootstrap/bootstrap-tagsinput.css")

<div style="margin-top: 10px">
    <div class="page-header">
        <h4>@PeriodicTaskStrings.periodictasks</h4>
        <h6></h6>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="tms-page-toolbar inline-toolbar left" data-block="#record">
                <div class="btn-group">
                    <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" id="btnNew"><i class="fa fa fa-plus fa-fw"></i> @PeriodicTaskStrings.addnew</button>
                    <button class="btn btn-default btn-sm" title="Ctrl+S" selection-required="false" id="btnSave"><i class="fa fa-floppy-o fa-fw"></i> @PeriodicTaskStrings.save</button>
                    <button class="btn btn-default btn-sm" title="Ctrl+U" selection-required="true" id="btnUndo"><i class="fa fa-undo fa-fw"></i> @PeriodicTaskStrings.undo</button>
                    <button class="btn btn-default btn-sm" title="Ctrl+D" selection-required="true" id="btnDelete"><i class="fa fa-trash-o fa-fw"></i> @PeriodicTaskStrings.delete</button>
                    <button class="btn btn-default btn-sm" title="Ctrl+H" selection-required="true" id="btnHistory" disabled="disabled"><i class="fa fa-bolt fa-fw"></i> @PeriodicTaskStrings.history</button>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            <div class="scrollbar-macosx">
                <div class="list-group"></div>
            </div>
        </div>
        <div class="col-md-9">
            <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" default-on-reset href="#record">@PeriodicTaskStrings.record</a></li>
                <li><a href="#taskparameters" enable-on-selection>@PeriodicTaskStrings.taskparameters</a></li>
                <li><a href="#activities" enable-on-selection>@PeriodicTaskStrings.activities</a></li>
                <li><a href="#preview" enable-on-selection>@PeriodicTaskStrings.preview</a></li>
            </ul>
            <div class="tab-content">
                <div id="record" class="tab-pane active">
                    <div class="panel panel-default">
                        <div class="panel-heading page-panel-heading">@PeriodicTaskStrings.record</div>
                        <div class="panel-body page-panel-body">
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.code
                                </div>
                                <div class="col-md-4">
                                    <input type="text" maxlength="50" id="code" required disableonupdate="yes" class="form-control form-code" />
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.description
                                </div>
                                <div class="col-md-4">
                                    <input type="text" maxlength="250" id="description" required class="form-control" />
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.organization
                                </div>
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <input type="text" maxlength="50" disableonupdate="yes" class="form-control" required id="organization">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btnorganization"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.type
                                </div>
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <input type="text" maxlength="50" class="form-control" required id="type">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button" id="btntype"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.tasktype
                                </div>
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <input type="text" maxlength="50" class="form-control" id="tasktype">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button" id="btntasktype"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.category
                                </div>
                                <div class="col-md-4">
                                    <div class="input-group">
                                        <input type="text" maxlength="50" class="form-control" required id="category">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" type="button" id="btncategory"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.frequency
                                </div>
                                <div class="col-md-4">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <input ctrltype="numeric" allowfloat="false" type="text" class="form-control" required id="frequency">
                                        </div>
                                        <div class="col-md-6">
                                            <select class="form-control" required id="frequencypart">
                                                <option value="D">@PeriodicTaskStrings.day</option>
                                                <option value="M">@PeriodicTaskStrings.month</option>
                                                <option value="Y">@PeriodicTaskStrings.year</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.priority
                                </div>
                                <div class="col-md-4">
                                    <select class="form-control" required id="priority"></select>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.upcomingtaskreminder
                                </div>
                                <div class="col-md-4">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <input type="text" ctrltype="numeric" allowfloat="false" maxlength="50" class="form-control" id="uctrfrequency">
                                        </div>
                                        <div class="col-md-6">
                                            <select class="form-control" id="uctrfrequencypart">
                                                <option value="D">@PeriodicTaskStrings.day</option>
                                                <option value="M">@PeriodicTaskStrings.month</option>
                                                <option value="Y">@PeriodicTaskStrings.year</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.autocreate
                                </div>
                                <div class="col-md-4">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled" id="autocreate" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.autocreationtime
                                </div>
                                <div class="col-md-4">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <input type="text" ctrltype="numeric" allowfloat="false" disabled="disabled" class="form-control" id="autocreationtime">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.ignoremondays
                                </div>
                                <div class="col-md-4">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled ignore" id="ignoremondays" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.ignoretuesdays
                                </div>
                                <div class="col-md-4">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled ignore" id="ignoretuesdays" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.ignorewednesdays
                                </div>
                                <div class="col-md-4">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled ignore" id="ignorewednesdays" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.ignorethursdays
                                </div>
                                <div class="col-md-4">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled ignore" id="ignorethursdays" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.ignorefridays
                                </div>
                                <div class="col-md-4">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled ignore" id="ignorefridays" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.ignoresaturdays
                                </div>
                                <div class="col-md-4">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled ignore" id="ignoresaturdays" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.ignoresundays
                                </div>
                                <div class="col-md-4">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled ignore" id="ignoresundays" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.ignoreofficialholidays
                                </div>
                                <div class="col-md-4">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled" id="ignoreofficialholidays" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-2">
                                    @PeriodicTaskStrings.active
                                </div>
                                <div class="col-md-4">
                                    <div class="checkbox checkbox-primary">
                                        <input type="checkbox" class="styled" checked="checked" id="active" />
                                        <label></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="taskparameters" class="tab-pane">
                    <div class="panel panel-default">
                        <div class="panel-heading page-panel-heading">@PeriodicTaskStrings.taskparameters</div>
                        <div id="grdPeriodicTaskParameters" style="border-width: 0px;"></div>
                        <div class="inline-toolbar left" data-block="#taskparameters">
                            <div class="btn-group">
                                <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" id="btnAddTaskParameter"><i class="fa fa fa-plus fa-fw"></i>@PeriodicTaskStrings.addnew</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+S" selection-required="false" id="btnSaveTaskParameter"><i class="fa fa-floppy-o fa-fw"></i>@PeriodicTaskStrings.save</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+D" selection-required="true" id="btnDeleteTaskParameter" disabled="disabled"><i class="fa fa-trash-o fa-fw"></i>@PeriodicTaskStrings.delete</button>
                            </div>
                        </div>
                        <div class="panel-body page-panel-body">
                            <div id="taskparameterdetails">
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.prmid
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" maxlength="50" id="prmid" disabled="disabled" class="form-control" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.prmlocation
                                    </div>
                                    <div class="col-md-4">
                                        <div class="input-group">
                                            <input type="text" maxlength="50" class="form-control" required id="prmlocation">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" id="btnprmlocation"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.prmequipment
                                    </div>
                                    <div class="col-md-4">
                                        <div class="input-group">
                                            <input type="text" maxlength="50" class="form-control" id="prmequipment">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" id="btnprmequipment"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.prmdepartment
                                    </div>
                                    <div class="col-md-4">
                                        <div class="input-group">
                                            <input type="text" maxlength="50" class="form-control" required id="prmdepartment">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" id="btnprmdepartment"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.prmbranch
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" maxlength="50" id="prmbranch" disabled class="form-control" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.trade
                                    </div>
                                    <div class="col-md-4">
                                        <div class="input-group">
                                            <input type="text" maxlength="50" class="form-control" id="prmtrade">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" id="btnprmtrade"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.prmplandate
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" ctrltype="datetimepicker" maxlength="50" id="prmplandate" required class="form-control" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.prmresponsible
                                    </div>
                                    <div class="col-md-4">
                                        <div class="input-group">
                                            <input type="text" maxlength="50" class="form-control" id="prmresponsible">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" id="btnprmresponsible"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.active
                                    </div>
                                    <div class="col-md-4">
                                        <div class="checkbox checkbox-primary">
                                            <input type="checkbox" class="styled" checked="checked" id="prmactive" />
                                            <label></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.workpermit
                                    </div>
                                    <div class="col-md-4">
                                        <div class="checkbox checkbox-primary">
                                            <input type="checkbox" class="styled" checked="checked" id="prmworkpermit" />
                                            <label></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.reporting
                                    </div>
                                    <div class="col-md-4">
                                        <div class="checkbox checkbox-primary">
                                            <input type="checkbox" class="styled" checked="checked" id="prmreporting" />
                                            <label></label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="activities" class="tab-pane">
                    <div class="panel panel-default">
                        <div class="panel-heading page-panel-heading">@PeriodicTaskStrings.activities</div>
                        <div id="grdPeriodicTaskActivities" style="border-width: 0px;"></div>
                        <div class="inline-toolbar left" data-block="#activities">
                            <div class="btn-group">
                                <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" id="btnAddActivity"><i class="fa fa fa-plus fa-fw"></i>@PeriodicTaskStrings.addnew</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+S" selection-required="false" id="btnSaveActivity"><i class="fa fa-floppy-o fa-fw"></i>@PeriodicTaskStrings.save</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+D" selection-required="true" id="btnDeleteActivity" disabled="disabled"><i class="fa fa-trash-o fa-fw"></i>@PeriodicTaskStrings.delete</button>
                            </div>
                        </div>
                        <div class="panel-body page-panel-body">
                            <div id="actdetails">
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.line
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" maxlength="50" id="line" disabled="disabled" class="form-control" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.taskdepartment
                                    </div>
                                    <div class="col-md-4">
                                        <div class="checkbox checkbox-primary">
                                            <input type="checkbox" class="styled" id="taskdepartment" />
                                            <label></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.department
                                    </div>
                                    <div class="col-md-4">
                                        <div class="input-group">
                                            <input type="text" maxlength="50" required class="form-control" id="dep">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" id="btnDepartment"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.status
                                    </div>
                                    <div class="col-md-4">
                                        <select id="status" class="form-control" required></select>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.taskdescription
                                    </div>
                                    <div class="col-md-4">
                                        <div class="checkbox checkbox-primary">
                                            <input type="checkbox" class="styled" id="taskdescription" />
                                            <label></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.description
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" maxlength="250" required id="actdesc" class="form-control" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.trade
                                    </div>
                                    <div class="col-md-4">
                                        <div class="input-group">
                                            <input type="text" maxlength="50" class="form-control" id="trade">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" id="btntrade"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.assignedto
                                    </div>
                                    <div class="col-md-4">
                                        <div class="input-group">
                                            <input type="text" data-role="multival" class="form-control" id="assignedto">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" id="btnAssignedTo"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.predecessor
                                    </div>
                                    <div class="col-md-4">
                                        <div class="input-group">
                                            <input type="text" class="form-control" id="predecessor">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" id="btnpredecessor"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.checklisttemplate
                                    </div>
                                    <div class="col-md-4">
                                        <div class="input-group">
                                            <input type="text" maxlength="50" class="form-control" id="checklisttmp">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" type="button" id="btnChecklistTmp"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.lmapprequired
                                    </div>
                                    <div class="col-md-4">
                                        <div class="checkbox checkbox-primary">
                                            <input type="checkbox" class="styled" id="lmapprovalrequired" />
                                            <label></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.hidden
                                    </div>
                                    <div class="col-md-8">
                                        <div class="checkbox checkbox-primary">
                                            <input type="checkbox" class="styled" id="hidden" />
                                            <label>
                                                <i class="fa fa-info-circle"></i> @PeriodicTaskStrings.ihidden
                                            </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.privateact
                                    </div>
                                    <div class="col-md-8">
                                        <div class="checkbox checkbox-primary">
                                            <input type="checkbox" class="styled" id="private" />
                                            <label>
                                                <i class="fa fa-info-circle"></i> @PeriodicTaskStrings.iprivate
                                            </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-2">
                                        @PeriodicTaskStrings.createseparateactivity
                                    </div>
                                    <div class="col-md-4">
                                        <div class="checkbox checkbox-primary">
                                            <input type="checkbox" class="styled" id="createseparateactivity" />
                                            <label></label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="preview" class="tab-pane">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="well">
                                <div class="row">
                                    <div class="col-md-1">
                                        <strong>@PeriodicTaskStrings.startdate</strong>
                                    </div>
                                    <div class="col-md-2">
                                        <input ctrltype="datepicker" maxlength="50" id="prmstart" required type="text" />
                                    </div>
                                    <div class="col-md-1">
                                        <strong>@PeriodicTaskStrings.enddate</strong>
                                    </div>
                                    <div class="col-md-2">
                                        <input ctrltype="datepicker" maxlength="50" id="prmend" required type="text" />
                                    </div>
                                    <div class="col-md-2">
                                        <button id="btnList" class="btn btn-info btn-xs">
                                            <i class="fa fa-eye"></i>
                                            @PeriodicTaskStrings.list
                                        </button>
                                        <button id="btnGenerate" class="btn btn-primary btn-xs">
                                            <i class="fa fa-save"></i>
                                            @PeriodicTaskStrings.generate
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div id="grdPeriodicTasksPreview"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal" id="confirmgenerate" role="dialog" data-easein="flipBounceYIn" aria-labelledby="modalsharedlbl" aria-hidden="true">
