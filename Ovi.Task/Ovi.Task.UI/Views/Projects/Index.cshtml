@using System.Globalization
@using Resources.Projects.Index
@{
    Layout = "~/Views/Shared/Default.cshtml";
}

<link href="~/Content/jquery/jquery.fileupload.css" rel="stylesheet" />
<link href="~/Content/jquery/jquery.contextMenu.min.css" rel="stylesheet" />
<div style="margin-top: 10px">
    <div class="page-header">
        <h4>
            @ProjectStrings.projects
        </h4>
        <h6></h6>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="tms-page-toolbar inline-toolbar left" data-block="#record">
                <div class="btn-group">
                    <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" id="btnNew"><i class="fa fa fa-plus fa-fw"></i>@ProjectStrings.addnew</button>
                    <button class="btn btn-default btn-sm" disabled="disabled" selection-required="false" title="Ctrl+S" id="btnSave"><i class="fa fa-floppy-o fa-fw"></i>@ProjectStrings.save</button>
                    <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+U" id="btnUndo"><i class="fa fa-undo fa-fw"></i>@ProjectStrings.undo</button>
                    <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+D" id="btnDelete"><i class="fa fa-trash-o fa-fw"></i>@ProjectStrings.delete</button>
                    <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+H" id="btnHistory"><i class="fa fa-bolt fa-fw"></i>@ProjectStrings.history</button>
                    <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+P" id="btnPrint"><i class="fa fa-print fa-fw"></i>@ProjectStrings.print</button>
                </div>
            </div>
        </div>
    </div>
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#list">@ProjectStrings.list</a></li>
        <li><a data-toggle="tab" href="#record">@ProjectStrings.record</a></li>
        <li><a enable-on-selection href="#tasks">@ProjectStrings.tasks</a></li>
        <li><a enable-on-selection href="#fixedcosts">@ProjectStrings.tabfixedcosts</a></li>
        @*<li><a enable-on-selection href="#quotation">@ProjectStrings.quotation</a></li>*@
    </ul>
    <div class="tab-content">
        <div id="list" class="tab-pane active">
            <div id="grdProjects"></div>
            <div class="sidebars">
                <div class="sidebar right" style="right: -400px;">
                    <div class="row">
                        <div class="col-md-12" style="text-align: center; margin-top: 10px;">
                            <button id="filter" class="btn btn-primary btn-xs"><i class="fa fa-filter"></i> @ProjectStrings.filter</button>
                            <button id="clearfilter" class="btn btn-info btn-xs"><i class="fa fa-ban"></i> @ProjectStrings.reset</button>
                            <button id="closesiderbar" class="btn btn-danger btn-xs"><i class="fa fa-times"></i> @ProjectStrings.close</button>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="page-header">
                                <h5>@ProjectStrings.status</h5>
                            </div>
                        </div>
                        <div class="col-md-12 filter-box" id="statuses">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="page-header">
                                <h5>@ProjectStrings.type</h5>
                            </div>
                        </div>
                        <div class="col-md-12 filter-box" id="types">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="page-header">
                                <h5>@ProjectStrings.datecreated</h5>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <input type="text" ctrltype="datepicker" class="i-filter form-control" id="datecreated_start">
                        </div>
                        <div class="col-sm-6">
                            <input type="text" ctrltype="datepicker" class="i-filter form-control" id="datecreated_end">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="tab-pane" id="record">
            <div class="row record-content">
                <div class="col-md-4" id="project">
                    <div class="panel panel-default" style="height:1010px;">
                        <div class="panel-heading task-panel-heading">@ProjectStrings.record</div>
                        <div class="panel-body task-panel-body">
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.projectid
                                </div>
                                <div class="col-md-8">
                                    <input id="projectid" type="text" disabled="disabled" class="form-control" />
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.description
                                </div>
                                <div class="col-md-8">
                                    <input type="text" disableoncomplete maxlength="250" class="form-control" required id="description" />
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.organization
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text"  loadstatusesonchange="yes" disableonupdate="yes" maxlength="50" required class="form-control uppercase" id="org">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btnorg"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.type
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text"  loadstatusesonchange="yes" disableonupdate="yes" maxlength="50" required data-entity="PROJECT" class="form-control uppercase" id="type">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btntype"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.status
                                </div>
                                <div class="col-md-8">
                                    <select class="form-control" data-code="-" data-pcode="Q" data-text="" required id="status"></select>
                                </div>
                            </div>
                            <div id="divcancellationreason" class="hidden">
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.cancellationreason
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" id="cancellationreason" disableoncomplete>
                                            <option class="default" value="">@ProjectStrings.pleaseselect</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.cancellationdesc
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" max="250" disableoncomplete class="form-control" id="cancellationdesc">
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.estimatedstart
                                </div>
                                <div class="col-md-8">
                                    <input type="text" disableoncomplete ctrltype="datepicker" class="form-control" id="estimatedstart">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.estimatedend
                                </div>
                                <div class="col-md-8">
                                    <input type="text" disableoncomplete ctrltype="datepicker" class="form-control" id="estimatedend">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.offerdeadline
                                </div>
                                <div class="col-md-8">
                                    <input type="text" disableoncomplete ctrltype="datetimepicker" class="form-control" id="offerdeadline">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.customer
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text"  disableonupdate="yes" maxlength="50" required class="form-control" id="customer">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableonupdate="yes" type="button" id="btncustomer"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                    <input type="text" maxlength="250" disabled="disabled" class="form-control" id="customerdesc">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.province
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text"  disableoncomplete required maxlength="50" class="form-control uppercase" id="province">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableoncomplete type="button" id="btnprovince"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.district
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text"  disableoncomplete maxlength="50" class="form-control uppercase" id="district">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableoncomplete type="button" id="btndistrict"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.region
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text"  disableoncomplete maxlength="50" class="form-control uppercase" id="region">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableoncomplete type="button" id="btnregion"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.channel
                                </div>
                                <div class="col-md-8">
                                    <select required="" class="form-control" disableoncomplete id="channel"></select>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.curr
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text"  disableoncomplete maxlength="50" class="form-control uppercase" id="currency">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableoncomplete type="button" id="btncurrency"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.exch
                                </div>
                                <div class="col-md-8">
                                    <input class="form-control" type="text" disableoncomplete valtype="EXCH" ctrltype="numeric" allowfloat="true" id="exch">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.paymentterm
                                </div>
                                <div class="col-md-8">
                                    <input type="text" max="250" disableoncomplete class="form-control" id="paymentterm">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.contactperson
                                </div>
                                <div class="col-md-8">
                                    <input type="text" max="250" disableoncomplete class="form-control" id="contactperson">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.contactphone
                                </div>
                                <div class="col-md-8">
                                    <input type="text" max="250" disableoncomplete ctrltype="phone" class="form-control" id="contactphone">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.contactemail
                                </div>
                                <div class="col-md-8">
                                    <input type="email" max="250" disableoncomplete class="form-control" id="contactemail">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.parent
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group">
                                        <input type="text" disableoncomplete maxlength="50" class="form-control" id="parent">
                                        <span class="input-group-btn">
                                            <button class="btn btn-default" disableoncomplete type="button" id="btnparent"><i class="fa fa-search fa-fw"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.totaltaskcount
                                </div>
                                <div class="col-md-8">
                                    <input type="text" disabled="disabled" class="form-control" id="totaltaskcount">
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.totalactivitycount
                                </div>
                                <div class="col-md-8">
                                    <input type="text" disabled="disabled" class="form-control" id="totalactivitycount" />
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.offerrevisioncount
                                </div>
                                <div class="col-md-8">
                                    <div style="display: inline-block; cursor: pointer;" id="offerrevisioncount"></div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.createdby
                                </div>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" disabled="disabled" id="createdby" />
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-4">
                                    @ProjectStrings.created
                                </div>
                                <div class="col-md-8">
                                    <input type="text" class="form-control" disabled="disabled" id="created" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div id="taskdetails">
                        <div class="panel panel-default">
                            <div class="panel-heading task-panel-heading">@ProjectStrings.taskdetails</div>
                            <div class="panel-body">
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.description
                                    </div>
                                    <div class="col-md-8">
                                        <input id="taskdescription" maxlength="80" disableoncomplete required type="text" class="form-control" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.department
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text" disableoncomplete  maxlength="50" required class="form-control uppercase" id="department">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disableoncomplete type="button" id="btndepartment"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.type
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text"  disableoncomplete maxlength="50" required data-entity="TASK" class="form-control" id="tasktype">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disableoncomplete type="button" id="btntasktype"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div><!-- /input-group -->
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.category
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text"  disableoncomplete maxlength="50" required class="form-control" id="category">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disableoncomplete type="button" id="btncategory"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div><!-- /input-group -->
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.branch
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text"  disableoncomplete maxlength="50" required class="form-control uppercase" id="branch">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disableoncomplete type="button" id="btnbranch"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.location
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text"  disableoncomplete maxlength="50" required class="form-control uppercase" id="location">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disableoncomplete type="button" id="btnlocation"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.priority
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" disableoncomplete required id="priority" data-selected="NORMAL">
                                            <option class="default" value="">@ProjectStrings.pleaseselect</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.requestedby
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" disabled="disabled" id="requestedby" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.daterequested
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" disabled="disabled" id="daterequested" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @ProjectStrings.deadline
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" ctrltype="datetimepicker" disableoncomplete class="form-control" id="deadline" />
                                    </div>
                                </div>
                                <div id="divTaskCustomFields" class="row custom">
                                    <div class="col-md-12">
                                        <div id="cftaskcontainer"></div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-12">
                                        <button type="button" disableoncomplete class="btn btn-primary btn-xs pull-right" id="btngeneratetask"><i class="fa fa-plus fa-fw" aria-hidden="true"></i> @ProjectStrings.generatetask</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="divCustomFields">
                        <div class="panel panel-default" style="height: 440px;">
                            <div class="panel-heading task-panel-heading">@ProjectStrings.customfields</div>
                            <div class="panel-body">
                                <div id="cfcontainer"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-4" id="divCmntsAttachments">
                    <div class="panel panel-default" style="height: 550px;">
                        <div class="panel-heading task-panel-heading">@ProjectStrings.comments</div>
                        <div class="panel-body">
                            <textarea class="form-control" contenteditable rows="3" id="comment"></textarea>
                            <div class="inline-toolbar">
                                <button class="btn btn-primary btn-xs" selection-required="true" id="addComment"><i class="fa fa-plus-circle"></i>@ProjectStrings.add</button>
                            </div>
                            <div id="comments" class="sm">
                            </div>
                        </div>
                    </div>
                    <div class="panel panel-default" style="height: 440px;">
                        <div class="panel-heading task-panel-heading">@ProjectStrings.docs</div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="progress">
                                        <div class="progress-bar" id="docuprogress" role="progressbar" style="width: 0%; min-width: 2em;">0%</div>
                                    </div>
                                </div>
                            </div>
                            <div class="row custom">
                                <div class="col-md-12">
                                    <div id="fupload">
                                        <span class="btn btn-default fileinput-button" id="btnBrowse">
                                            <i class="fa fa-search"></i>
                                            <span>@ProjectStrings.browse</span>
                                            <input type="file" name="files[]" id="fu" multiple data-url="/UploadFile.ashx">
                                        </span>
                                        <button class="btn btn-default" id="btnupload" disabled="disabled"><i class="fa fa-cloud-upload"></i>@ProjectStrings.upload</button>
                                        <div class="row custom">
                                            <div class="col-md-12">
                                                <div id="filename"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div id="docs" class="light-size"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="tasks" class="tab-pane">
            <div id="grdProjectTasks"></div>
        </div>
        <div id="fixedcosts" class="tab-pane">
            <div class="row">
                <div class="col-md-12">
                    <div id="grdFixedCosts"></div>
                </div>
            </div>
            <div id="fixedcostsform" class="panel panel-default items">
                <div class="panel-heading page-panel-heading">@ProjectStrings.tabfixedcosts</div>
                <div class="row">
                    <div class="col-md-12" style="padding-right: 0">
                        <div class="inline-toolbar left" style="border: none;" data-block="#fixedcosts">
                            <div class="btn-group">
                                <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" disabled id="btnAddFixedCost"><i class="fa fa fa-plus fa-fw"></i>@ProjectStrings.addnew</button>
                                <button class="btn btn-default btn-sm" title="Ctrl+S" selection-required="false" disabled id="btnSaveFixedCost"><i class="fa fa-floppy-o fa-fw"></i>@ProjectStrings.save</button>
                                <button class="btn btn-default btn-sm" selection-required="true" title="Ctrl+D" disabled id="btnDeleteFixedCost"><i class="fa fa-trash-o fa-fw"></i>@ProjectStrings.delete</button>
                            </div>
                        </div>
                    </div>
                    <div class="panel-body page-panel-body">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="row custom">
                                    <div class="col-md-3">
                                        @ProjectStrings.date
                                    </div>
                                    <div class="col-md-5">
                                        <input type="text" calc-group="2" maxlength="10" class="form-control" required ctrltype="datepicker" id="fixedcostdate" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-3">
                                        @ProjectStrings.description
                                    </div>
                                    <div class="col-md-5">
                                        <input type="text" maxlength="250" class="form-control" required id="fixedcostdesc" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-3">
                                        @ProjectStrings.type
                                    </div>
                                    <div class="col-md-4">
                                        <select id="typ" class="form-control" required>
                                            <option value="">@ProjectStrings.pleaseselect</option>
                                            <option value="PART">@ProjectStrings.part</option>
                                            <option value="SERVICE">@ProjectStrings.service</option>
