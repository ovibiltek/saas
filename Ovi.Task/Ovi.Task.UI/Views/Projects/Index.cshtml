<div class="modal" id="modalprint" role="dialog" aria-labelledby="modalsharedlbl" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <form action="/Reports/GenerateProjectOutput" method="POST" target="_blank" novalidate>
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="false">&times;</span></button>
                    <h4 class="modal-title">@ProjectStrings.printoptions</h4>
                </div>
                <div class="modal-body">
                    <div class="row custom">
                        <div class="col-md-4">
                            @ProjectStrings.printdetailed
                        </div>
                        <div class="col-md-8">
                            <input type="checkbox" id="printdetailed" name="PrintDetailed" value="+" />
                        </div>
                    </div>
                    <div class="row custom">
                        <div class="col-md-4">
                            @ProjectStrings.outputtype
                        </div>
                        <div class="col-md-8">
                            <div class="radio radio-primary radio-inline">
                                <input type="radio" name="Type" id="pdf" value="PDF" disableonupdate="yes" checked>
                                <label for="pdf">@ProjectStrings.pdf</label>
                            </div>
                            <div class="radio radio-primary radio-inline">
                                <input type="radio" name="Type" id="xls" disableonupdate="yes" value="XLS">
                                <label for="xls">@ProjectStrings.xls</label>
                            </div>
                        </div>
                    </div>
                    <input type="hidden" name="ProjectId" id="ProjectId">
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary btn-xs"><i class="fa fa-fw fa-print"></i> @ProjectStrings.print</button>
                </div>
            </form>
        </div>
    </div>
</div>
