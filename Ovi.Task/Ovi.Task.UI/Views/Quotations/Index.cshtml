@using Resources.Quotations.Index
@using Resources.Shared.Index
@{
    Layout = "~/Views/Shared/Default.cshtml";
}

@Html.IncludeVersionedCss("/Content/bootstrap/bootstrap-tagsinput.css")
@Html.IncludeVersionedCss("/Content/jquery/jquery.fileupload.css")
@Html.IncludeVersionedCss("/Content/InfoTooltip.css")
@Html.IncludeVersionedCss("/Content/jquery/jquery.contextMenu.min.css")

@{
    // Web.config'den değer alınır
    var hideCustomer = ConfigHelper.Get("HideCustomer");
    var hideSalesSection= ConfigHelper.Get("HideSalesSection");

    var isCustomerVisible = !(hideCustomer == "+"); // Eğer "+" ise görünür olacak, değilse gizlenecek
    var isSalesSectionVisible = !(hideSalesSection == "+"); // Eğer "+" ise görünür olacak, değilse gizlenecek


}

<div style="margin-top: 10px">
    <div class="page-header">
        <h4>
            @QuotationStrings.quotations
        </h4>
        <h6></h6>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="tms-page-toolbar inline-toolbar left" data-block="#record">
                <div class="tms-page-toolbar inline-toolbar left" data-block="#record">
                    <div class="btn-group">
                        <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" id="btnNew"><i class="fa fa fa-plus fa-fw"></i>@QuotationStrings.addnew</button>
                        <button class="btn btn-default btn-sm" disabled="disabled" selection-required="false" title="Ctrl+S" id="btnSave"><i class="fa fa-floppy-o fa-fw"></i>@QuotationStrings.save</button>
                        <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+U" id="btnUndo"><i class="fa fa-undo fa-fw"></i>@QuotationStrings.undo</button>
                        <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+D" id="btnDelete"><i class="fa fa-trash-o fa-fw"></i>@QuotationStrings.delete</button>
                        <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+H" id="btnHistory"><i class="fa fa-bolt fa-fw"></i>@QuotationStrings.history</button>
                        @if (string.IsNullOrEmpty(UserManager.Instance.User.Supplier))
                        {
                            <button class="btn btn-default btn-sm" disabled="disabled" selection-required="true" title="Ctrl+P" id="btnPrint"><i class="fa fa-print fa-fw"></i> @QuotationStrings.print</button>
                        }
                    </div>
                </div>
            </div>
        </div>
        <ul class="nav nav-tabs" id="navquotations">
            <li class="active"><a data-toggle="tab" href="#list">@QuotationStrings.list</a></li>
            <li><a data-toggle="tab" href="#record">@QuotationStrings.record</a></li>
            @if (string.IsNullOrEmpty(UserManager.Instance.User.Supplier) && string.IsNullOrEmpty(UserManager.Instance.User.Customer))
            {
                <li><a enable-on-selection href="#quotationlabors">@QuotationStrings.labor</a></li>
                <li><a enable-on-selection href="#quotationparts">@QuotationStrings.part</a></li>
            }
            @if (string.IsNullOrEmpty(UserManager.Instance.User.Customer))
            {
                <li><a enable-on-selection href="#quotationmisccosts">@QuotationStrings.misccosts</a></li>
            }
            @if (string.IsNullOrEmpty(UserManager.Instance.User.Supplier))
            {
                if (isSalesSectionVisible)
                {
                    <li><a enable-on-selection href="#quotationsales">@QuotationStrings.quotationsales</a></li>
                }
                if (string.IsNullOrEmpty(UserManager.Instance.User.Customer))
                {
                    <li><a enable-on-selection href="#quotationsum">@QuotationStrings.summary</a></li>
                }
            }
        </ul>
        <div class="tab-content">
            <div id="list" class="tab-pane active">
                <div class="row">
                    <div class="col-md-12">
                        <div id="grdQuotations"></div>
                        <div class="sidebars">
                            <div class="sidebar right" style="right: -400px;">
                                <div class="row">
                                    <div class="col-md-12" style="text-align: center; margin-top: 10px;">
                                        <button id="filter" class="btn btn-primary btn-xs"><i class="fa fa-filter"></i> @QuotationStrings.filter</button>
                                        <button id="clearfilter" class="btn btn-info btn-xs"><i class="fa fa-ban"></i> @QuotationStrings.reset</button>
                                        <button id="closesiderbar" class="btn btn-danger btn-xs"><i class="fa fa-times"></i> @QuotationStrings.close</button>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-12">
                                        <div class="page-header">
                                            <h5>@QuotationStrings.datecreated</h5>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <input type="text" ctrltype="datepicker" class="i-filter form-control" id="datecreated_start">
                                    </div>
                                    <div class="col-md-6">
                                        <input type="text" ctrltype="datepicker" class="i-filter form-control" id="datecreated_end">
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-12">
                                        <div class="page-header">
                                            <h5>@QuotationStrings.appdate</h5>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <input type="text" ctrltype="datepicker" class="i-filter form-control" id="appdate_start">
                                    </div>
                                    <div class="col-md-6">
                                        <input type="text" ctrltype="datepicker" class="i-filter form-control" id="appdate_end">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="record" class="tab-pane">
                <div class="row record-content">
                    <div class="col-md-4" id="divHeader">
                        <div class="panel panel-default" style="height: 842px;">
                            <div class="panel-heading task-panel-heading">@QuotationStrings.record</div>
                            <div class="panel-body task-panel-body">
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.code
                                    </div>
                                    <div class="col-md-8">
                                        <input id="code" disabled="disabled" alwaysdisabled="yes" type="text" class="form-control uppercase" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.organization
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text" loadstatusesonchange="yes" disableonupdate="yes" maxlength="50" required class="form-control uppercase" id="org">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disableonupdate="yes" type="button" id="btnorg"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.description
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" disableoncomplete maxlength="250" class="form-control" required id="desc" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <!--Not-->
                                    <div class="col-md-4">
                                        @QuotationStrings.note
                                    </div>
                                    <div class="col-md-8">
                                        <textarea class="form-control" required disableoncomplete maxlength="1000" contenteditable="" rows="3" id="quonote" autocomplete="off" tabindex="19"></textarea>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.type
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text" loadstatusesonchange="yes" disableonupdate="yes" maxlength="50" required data-entity="QUOTATION" class="form-control uppercase" id="type">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disableonupdate="yes" type="button" id="btntype"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                @if (string.IsNullOrEmpty(UserManager.Instance.User.Customer))
                                {
                                    <div id="divsupplier" class="row custom hidden">
                                        <div class="col-md-4">
                                            @QuotationStrings.supplier
                                        </div>
                                        <div class="col-md-8">
                                            <div class="input-group">
                                                <input type="text" disableonupdate="yes" maxlength="50" class="form-control uppercase" id="supplier">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-default" disableonupdate="yes" type="button" id="btnsupplier"><i class="fa fa-search fa-fw"></i></button>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                }
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.status
                                    </div>
                                    <div class="col-md-8">
                                        <select class="form-control" data-code="-" data-pcode="Q" data-text="" required id="status"></select>
                                    </div>
                                </div>
                                <div id="divcancellationreason" class="hidden">
                                    <div class="row custom">
                                        <div class="col-md-4">
                                            @QuotationStrings.cancellationreason
                                        </div>
                                        <div class="col-md-8">
                                            <select class="form-control" disableoncomplete id="cancellationreason">
                                                <option class="default" value="">@QuotationStrings.pleaseselect</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div id="divrejectreason" class="hidden">
                                    <!-- Müşteri tarafından red edildi.-->
                                    <div class="row custom">
                                        <div class="col-md-4">
                                            @QuotationStrings.rejectreason
                                        </div>
                                        <div class="col-md-8">
                                            <div class="input-group">
                                                <input type="text" maxlength="50" class="form-control uppercase" id="rejectreason">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-default" type="button" id="btnrejectreason"><i class="fa fa-search fa-fw"></i></button>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.revision
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" disabled="disabled" maxlength="50" class="form-control" id="revision" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.referenceno
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" disableoncomplete maxlength="50" class="form-control" id="referenceno" />
                                    </div>
                                </div>
                                <div class="row custom" id="divCustomer" style="@(isCustomerVisible ?  "" : "display:none;" )">
                                    <div class="col-md-4">
                                        @QuotationStrings.customer
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text" disableonupdate="yes" maxlength="50" required class="form-control uppercase" id="customer">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disableonupdate="yes" type="button" id="btncustomer"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div id="taskactivitydiv">
                                    <div class="row custom">
                                        <div class="col-md-4">
                                            @QuotationStrings.task
                                        </div>
                                        <div class="col-md-8">
                                            <div class="input-group">
                                                <input type="text" disableoncomplete maxlength="50" class="form-control" id="task">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-default" disableoncomplete type="button" id="btntask"><i class="fa fa-search fa-fw"></i></button>
                                                </span>
                                            </div>
                                            <input type="text" maxlength="250" disabled="disabled" class="form-control" id="taskdesc">
                                        </div>
                                    </div>
                                    <div class="row custom">
                                        <div class="col-md-4">
                                            @QuotationStrings.branch
                                        </div>
                                        <div class="col-md-8">
                                            <input type="text" maxlength="250" disabled="disabled" class="form-control" id="branch">
                                        </div>
                                    </div>
                                    <div class="row custom">
                                        <div class="col-md-4">
                                            @QuotationStrings.location
                                        </div>
                                        <div class="col-md-8">
                                            <input type="text" maxlength="250" disabled="disabled" class="form-control" id="location">
                                        </div>
                                    </div>
                                    <div class="row custom">
                                        <div class="col-md-4">
                                            @QuotationStrings.activity
                                        </div>
                                        <div class="col-md-8">
                                            <select class="form-control" disableoncomplete disabled="disabled" id="activity">
                                                <option class="default" value="">@QuotationStrings.pleaseselect</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div id="projectdiv" class="hidden">
                                    <div class="row custom">
                                        <div class="col-md-4">
                                            @QuotationStrings.project
                                        </div>
                                        <div class="col-md-8">
                                            <div class="input-group">
                                                <input type="text" disableoncomplete maxlength="50" class="form-control" id="project">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-default" disableoncomplete type="button" id="btnproject"><i class="fa fa-search fa-fw"></i></button>
                                                </span>
                                            </div>
                                            <input type="text" maxlength="250" disabled="disabled" class="form-control" id="projectdesc">
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.supplyperiod
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" disableoncomplete ctrltype="datepicker" class="form-control" id="supplyperiod">
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.remindingperiod
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" disableoncomplete ctrltype="numeric" allowfloat="false" maxlength="250" class="form-control" id="remindingperiod">
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.paymentdue
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" disableoncomplete ctrltype="numeric" allowfloat="false" maxlength="250" class="form-control" id="paymentdue">
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.currency
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text" disableoncomplete required maxlength="50" class="form-control" id="curr">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disableoncomplete type="button" id="btncurr"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.exch
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" required valtype="EXCH" ctrltype="numeric" allowfloat="true" class="form-control" id="exch">
                                    </div>
                                </div>
                                <div class="row hidden custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.manager
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text" disabled="disabled" maxlength="50" class="form-control uppercase" id="manager">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disabled="disabled" type="button" id="btnmanager"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.validityperiod
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" required valtype="QTY" ctrltype="numeric" allowfloat="false" class="form-control" id="validityperiod">
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.cusmail
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" disableoncomplete maxlength="300" class="form-control" id="cusmail" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.mailrecipients
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input type="text" disableoncomplete class="form-control" data-role="multival" id="mailrecipients">
                                            <span class="input-group-btn">
                                                <button class="btn btn-default" disableoncomplete type="button" id="btnmailrecipients"><i class="fa fa-search fa-fw"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.createdby
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" disabled="disabled" id="createdby" />
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-4">
                                        @QuotationStrings.created
                                    </div>
                                    <div class="col-md-8">
                                        <input type="text" class="form-control" disabled="disabled" id="created" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4" id="divCustomFields">
                        <div class="panel panel-default" style="height: 842px;">
                            <div class="panel-heading task-panel-heading">@QuotationStrings.customfields</div>
                            <div class="panel-body">
                                <div id="cfcontainer"></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4" id="divCmntsAttachments">
                        <div class="panel panel-default" style="height: 450px;">
                            <div class="panel-heading task-panel-heading">@QuotationStrings.comments</div>
                            <div class="panel-body">
                                <textarea class="form-control" contenteditable rows="3" id="comment"></textarea>
                                @if (string.IsNullOrEmpty(UserManager.Instance.User.Customer))
                                {
                                    <div class="row custom">
                                        <div class="col-md-12">
                                            <div class="checkbox checkbox-inline checkbox-primary">
                                                <input type="checkbox" class="styled" id="visibletocustomer" />
                                                <label>@SharedStrings.visibletocustomer</label>
                                            </div>
                                            @if (string.IsNullOrEmpty(UserManager.Instance.User.Supplier))
                                            {
                                                <div class="checkbox  checkbox-inline checkbox-primary">
                                                    <input type="checkbox" class="styled" id="visibletosupplier" />
                                                    <label>@SharedStrings.visibletosupplier</label>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                }
                                <div class="inline-toolbar">
                                    <button class="btn btn-primary btn-xs" selection-required="true" id="addComment"><i class="fa fa-plus-circle"></i>@QuotationStrings.add</button>
                                </div>
                                <div id="comments" class="sm">
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default" style="height: 372px;">
                            <div class="panel-heading task-panel-heading">@QuotationStrings.docs</div>
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="progress">
                                            <div class="progress-bar" id="docuprogress" role="progressbar" style="width: 0%; min-width: 2em;">0%</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row custom">
                                    <div class="col-md-12">
                                        <div id="fupload">
                                            <span class="btn btn-default fileinput-button" id="btnBrowse">
                                                <i class="fa fa-search"></i>
                                                <span>@QuotationStrings.browse</span>
                                                <input type="file" name="files[]" id="fu" multiple data-url="/UploadFile.ashx">
                                            </span>
                                            <button class="btn btn-default" id="btnupload" disabled="disabled"><i class="fa fa-cloud-upload"></i>@QuotationStrings.upload</button>
                                            @if (string.IsNullOrEmpty(UserManager.Instance.User.Supplier))
                                            {
                                                <button class="btn btn-default" id="btnDownload" disabled="disabled"><i class="fa fa-download" aria-hidden="true"></i> @QuotationStrings.download</button>
                                            }
                                            <div class="row custom">
                                                <div class="col-md-12">
                                                    <div id="filename"></div>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="docs" class="light-size"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            @if (string.IsNullOrEmpty(UserManager.Instance.User.Supplier) && string.IsNullOrEmpty(UserManager.Instance.User.Customer))
            {
                <div id="quotationlabors" class="tab-pane">
                    <div class="row">
                        <div class="col-md-12">
                            <div id="grdQuotationLabors"></div>
                        </div>
                    </div>
                    <br />
                    <div class="panel panel-default" id="quotationlaborsform">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="panel-body page-panel-body">
                                    <div class="row">
                                        <div class="col-md-12" style="padding-right:0">
                                            <div class="inline-toolbar left" style="border:none;" data-block="#exch">
                                                <div class="btn-group">
                                                    <button class="btn btn-default btn-sm" title="Ctrl+R" selection-required="false" id="btnAddQuotationLabor"><i class="fa fa fa-plus fa-fw"></i>@QuotationStrings.addnew</button>
                                                    <button class="btn btn-default btn-sm" title="Ctrl+S" selection-required="false" id="btnSaveQuotationLabor"><i class="fa fa-floppy-o fa-fw"></i>@QuotationStrings.save</button>
                                                    <button class="btn btn-default btn-sm" title="Ctrl+D" selection-required="true" id="btnDeleteQuotationLabor" disabled="disabled"><i class="fa fa-trash-o fa-fw"></i>@QuotationStrings.delete</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="panel panel-default">
                                                <div class="panel-heading">@QuotationStrings.labordetails</div>
                                                <div class="panel-body">
                                                    <div class="row custom">
                                                        <div class="col-md-4">
                                                            @QuotationStrings.laborid
                                                        </div>
                                                        <div class="col-md-8">
                                                            <input disabled="disabled" alwaysdisabled="yes" id="laborid" type="text" class="form-control" />
                                                        </div>
                                                    </div>
                                                    <div class="row custom">
                                                        <div class="col-md-4">
                                                            @QuotationStrings.servicecode
                                                        </div>
                                                        <div class="col-md-8">
                                                            <div class="input-group">
                                                                <input type="text" class="form-control" required="required" id="laborservicecode">
                                                                <span class="input-group-btn">
                                                                    <button class="btn btn-default" type="button" id="btnlaborservicecode"><i class="fa fa-search fa-fw"></i></button>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row custom">
                                                        <div class="col-md-4">
                                                            @QuotationStrings.period
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="text" required="" ctrltype="numeric" calc-group="3" allowfloat="true" class="form-control" id="laborperiod" value="1">
                                                        </div>
                                                        <div class="col-md-4">
